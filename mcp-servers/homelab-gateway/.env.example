# Server
NODE_ENV=production
PORT=3500

# n8n — workflow automation
N8N_API_URL=http://n8n:5678
N8N_API_KEY=your-n8n-api-key

# n8n Webhooks (external URL for webhook triggers)
N8N_WEBHOOK_BASE=https://n8n.your-domain.com/webhook

# Qdrant — vector database for memory/embeddings
QDRANT_URL=http://Qdrant:6333

# Neo4j — knowledge graph
NEO4J_URL=bolt://neo4j:7687
NEO4J_USER=neo4j
NEO4J_PASSWORD=your-neo4j-password

# PostgreSQL / pgvector — relational DB + vector search
POSTGRES_HOST=pgvector
POSTGRES_PORT=5432
POSTGRES_USER=postgresql
POSTGRES_PASSWORD=your-postgres-password
POSTGRES_DB=postgres_db

# Docker — container management via socket proxy
DOCKER_HOST=http://docker-socket-proxy:2375

# HashiCorp Vault — secrets management
VAULT_ADDR=http://Vault:8200
VAULT_TOKEN=your-vault-token
VAULT_MOUNT=homelab
VAULT_UNSEAL_KEY=your-vault-unseal-key

# OpenRouter — LLM API access
OPENROUTER_API_KEY=your-openrouter-api-key

# GitHub — repository operations
GITHUB_TOKEN=your-github-token

# Dify — AI application platform
DIFY_API_URL=http://dify-api:5001
DIFY_EMAIL=your-dify-email
DIFY_PASSWORD=your-dify-password

# OpenHands — AI coding agent
OPENHANDS_URL=http://openhands:3000
OPENHANDS_API_KEY=your-openhands-api-key

# Uptime Kuma — service monitoring (via REST API wrapper)
UPTIME_KUMA_API_URL=http://Uptime-Kuma-API:8000
UPTIME_KUMA_API_USER=your-uptime-kuma-username
UPTIME_KUMA_API_PASS=your-uptime-kuma-password
UPTIME_KUMA_API_KEY=your-uptime-kuma-api-key

# Traefik — reverse proxy inspection
TRAEFIK_API_URL=http://traefik:8080

# Redis — cache and message broker
REDIS_HOST=Redis
REDIS_PORT=6379
REDIS_PASSWORD=

# MongoDB — document database
MONGODB_URL=mongodb://MongoDB:27017

# Prometheus — metrics collection (defaults to http://prometheus:9090)
# PROMETHEUS_URL=http://prometheus:9090

# RabbitMQ — message queue
# RABBITMQ_URL=http://RabbitMQ:15672
# RABBITMQ_USER=guest
# RABBITMQ_PASS=guest

# ntfy — push notifications
NTFY_URL=http://ntfy:80
NTFY_TOPIC=homelab-projects
